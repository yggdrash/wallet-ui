{
  "name": "yggdrash-wallet",
  "version": "0.0.1",
  "license": "MIT",
  "productName": "Yggdrash Client",
  "description": "Yggdrash Desktop Wallet",
  "keywords": [
    "Yggdrash",
    "Wallet",
    "Yeed",
    "R2V",
    "Client",
    "Electron",
    "React"
  ],
  "private": false,
  "scripts": {
    "start": "concurrently --kill-others \" yarn run startReact \" \"ENV=dev electron .\"",
    "startReact": "cd client && npm start",
    "build": "electron-packager . --prune=false --icon=ygg-logo.icns --ignore=/client --overwrite --out=dist",
    "prebuild": "yarn run deleteWallet && cd client && npm run build",
    "deleteWallet": "rm -rf yggdrash-chain/src/accounts/privateKey"
  },
  "main": "main.js",
  "devDependencies": {
    "concurrently": "^3.6.0",
    "electron": "^2.0.3",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "about-window": "^1.11.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "electron-window-state": "^4.1.1",
    "elliptic": "^6.4.0",
    "express": "^4.16.2",
    "get-port": "^3.2.0",
    "global": "^4.3.2",
    "hex-to-binary": "^1.0.1",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-icons-kit": "^1.1.5",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.3",
    "styled-reset": "^1.3.5",
    "typography": "^0.16.17",
    "ws": "^4.1.0"
  }
}
